@model Mvc_RazorViewEngine.Models.PersonelSalesVM

<h1>@Model.Personel.Id</h1>
<h1>@Model.Personel.Name</h1>
<h1>@Model.Personel.Surname</h1>

<table border="1">
    <thead>
        <th>Ürün Adı </th>
        <th>Fiyatı </th>
    </thead>
    <tbody>
        @{
            for (int i = 0; i < Model.Sales.Count; i++)
            {
                var sales = Model.Sales[i];
                <tr>
             <td>@sales.ProductName</td>
             <td>@sales.Price</td>
             </tr>   
            }
        }
    </tbody>
</table>